https://leetcode.com/problems/length-of-last-word/submissions/



class Solution {
    public int lengthOfLastWord(String s) {
        String arr[]= null;//new String[s.length()];
       // for(int i=0;i<s.length();i++){
            arr=s.split(" ");
        //}
        String ch = arr[arr.length-1];
        int a = ch.length();
        return a;
    }
}
